{
  "configurations": [
    {
      "name": "x64-Debug",
      "generator": "Ninja",
      "configurationType": "Debug",
      "inheritEnvironments": [ "msvc_x64_x64" ],
      "buildRoot": "${workspaceRoot}\\..\\build\\${workspaceHash}\\${name}",
      "installRoot": "${workspaceRoot}\\..\\build\\${workspaceHash}\\install\\${name}",
      "cmakeCommandArgs": "",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": "",
      "variables": [
        {
          "name": "CMAKE_TOOLCHAIN_FILE",
          "value": "${env.VCPKG_ROOT}\\scripts\\buildsystems\\vcpkg.cmake",
          "type": "STRING"
        },
        {
          "name": "CMAKE_EXPORT_COMPILE_COMMANDS",
          "value": "True",
          "type": "BOOL"
        }
      ]
    }
  ]
}